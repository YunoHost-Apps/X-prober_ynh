{
    "name": "x-prober",
    "id": "x-prober",
    "packaging_format": 1,
    "description": {
        "en": "Probe program for PHP environment",
        "fr": "Programme de test pour l'environnement PHP"
    },
    "version": "8.16~ynh1",
    "url": "https://prober.inn-studio.com/",
    "upstream": {
        "license": "GPL-3.0",
        "website": "https://prober.inn-studio.com/",
        "demo": "https://prober.inn-studio.com/",
        "userdoc": "https://github.com/kmvan/x-prober",
        "code": "https://github.com/kmvan/x-prober"
    },
    "license": "GPL-3.0",
    "maintainer": {
        "name": "jarod5001",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php8.0-fpm",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/xprober",
                "default": "/xprober"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            } 
        ]
    }
}
